= form_for([@sprint, @working]) do |f|
  - if @working.errors.any?
    #error_explanation
      %h2
        = pluralize(@working.errors.count, "error")
        prohibited this working from being saved:
      %ul
        - @working.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :day
    .datepicker
      = f.text_field :day
  .actions
    = f.submit
